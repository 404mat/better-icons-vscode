{
	"name": "better-icons",
	"displayName": "Better Icons",
	"description": "Better Icons for VSCode",
	"version": "1.0",
	"publisher": "404mat",
	"pricing": "Free",
	"license": "Apache-2.0",
	"author": {
		"name": "Mathias D"
	},
	"icon": "logo.png",
	"engines": {
		"node": ">=16",
		"vscode": "^1.92.0"
	},
	"preview": false,
	"homepage": "https://github.com/moxer-theme/moxer-icons-code",
	"repository": {
		"type": "git",
		"url": "https://github.com/moxer-theme/moxer-icons-code.git"
	},
	"galleryBanner": {
		"color": "#090A0F",
		"theme": "dark"
	},
	"bugs": {
		"url": "https://github.com/moxer-theme/moxer-icons-code/issues"
	},
	"categories": [
		"Themes",
		"Other"
	],
	"keywords": [
		"theme",
		"icon-theme",
		"icons",
		"better",
		"colorful",
		"grayscale",
		"gray",
		"moxer"
	],
	"extensionKind": [
		"ui",
		"workspace"
	],
	"contributes": {
		"iconThemes": [
			{
				"id": "better-icons",
				"label": "Better Icons",
				"path": "./build/better-icons.json"
			},
			{
				"id": "better-icons-grayscale",
				"label": "Better Icons - Grayscale",
				"path": "./build/better-icons-grayscale.json"
			}
		]
	},
	"scripts": {
		"lint": "tslint --fix -c tslint.json 'src/**/*.ts'",
		"precommit": "run-s lint",
		"prebuild": "rm -rf build && tsc src/index.ts && run-s copy:icons",
		"build": "node src/index.js",
		"copy:icons": "mkdir -p build/svgs && cp -R src/svgs build && mkdir -p build/svgs-grayscale && cp -R src/svgs-grayscale build",
		"release": "git pull && standard-version --release-as patch && git push --follow-tags",
		"release:minor": "git pull && standard-version --release-as minor && git push --follow-tags",
		"release:major": "git pull && standard-version --release-as major && git push --follow-tags"
	},
	"devDependencies": {
		"@types/fs-extra": "11.0.4",
		"@types/node": "^20.11.5",
		"npm-run-all": "4.1.5",
		"tslint": "5.20.1",
		"tslint-config-prettier": "1.18.0",
		"typescript": "5.3.3",
		"vscode": "1.1.35"
	},
	"dependencies": {
		"fs-extra": "11.2.0",
		"prettier": "3.2.4",
		"standard-version": "9.5.0"
	}
}
